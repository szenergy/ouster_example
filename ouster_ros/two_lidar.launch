<!--
An example how to use two Ouster together.
In this case the computer is 192.168.1.100, the left Ouster is 192.168.1.204, the right is 205.
(This also works with os1-990123456789.local instead of 192.168.x.x)
The published topcs in ths case:
    /left/os1_cloud_node/imu
    /left/os1_cloud_node/points
    /left/os1_node/imu_packets
    /left/os1_node/lidar_packets
    /right/os1_cloud_node/imu
    /right/os1_cloud_node/points
    /right/os1_node/imu_packets
    /right/os1_node/lidar_packets
-->
<launch>
    <!-- assuming the two lidars are 1.16 m apart from each other -->
    <node pkg="tf" type="static_transform_publisher" name="two_ousters" args="0.0 1.16 0.0 0.0 0.0 0.0 left/os1_lidar right/os1_lidar 100" />
    
    <group ns="left">
            <include file="$(find ouster_ros)/os1.launch" >
                <arg name="os1_hostname" value="192.168.1.204"/>
                <arg name="os1_udp_dest" value="192.168.1.100"/>
                <arg name="os1_lidar_port" default="7502" doc="port to which the sensor should send lidar data"/>
                <arg name="os1_imu_port" default="7503" doc="port to which the sensor should send imu data"/>
                <arg name="tf_prefix" default="left"/>
            </include>
    </group>
    <group ns="right">
            <include file="$(find ouster_ros)/os1.launch" >
                <arg name="os1_hostname" value="192.168.1.205"/>
                <arg name="os1_udp_dest" value="192.168.1.100"/>
                <arg name="os1_lidar_port" default="7504" doc="port to which the sensor should send lidar data"/>
                <arg name="os1_imu_port" default="7505" doc="port to which the sensor should send imu data"/>
                <arg name="tf_prefix" default="right"/>
            </include>
    </group>
</launch>